	<groupId>com.cu.thesis</groupId>
	<artifactId>WuMuBPMN</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>WuMuBPMN</name>
	<description>Weak Mutation Testing tool for BPMN</description>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
