			<groupId>org.camunda.bpm.model</groupId>
			<artifactId>camunda-bpmn-model</artifactId>
			<version>7.8.0</version>
		</dependency>

		
		<!-- http://roufid.com/3-ways-to-add-local-jar-to-maven-project/#2-_adding_directly_the_dependency_as_system_scope -->
		<!-- ${basedir} represents the directory containing pom.xml. -->
		<dependency>
			<groupId>WuMuBPMNFonts</groupId>
			<artifactId>WuMuBPMNFonts</artifactId>
			<version>1.0</version>
			<scope>system</scope>
			<systemPath>${basedir}\\src\\main\\resources\\lib\\WuMuBPMNFonts.jar</systemPath>
		</dependency>
